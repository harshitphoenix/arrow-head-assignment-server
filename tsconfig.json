{
    "extends": "@tsconfig/node16/tsconfig.json",
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist"
    },
    "include": [
        "./*.ts",
        "./src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}